    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "firebase-admin": "^11.11.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.4",
    "openai": "^4.104.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
